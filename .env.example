### env.example for https://github.com/growthepie/gtp-frontend repository ###
NEXT_PUBLIC_VERCEL_ENV=development
NEXT_PUBLIC_VERCEL_URL=localhost:3000

# Domain for production deployment (format: www.example.com) — is different across www., fees., and labels. deployments
PRODUCTION_DOMAIN=

# Token included in the header of all requests to the backend endpoints during local development — used to bypass AWS WAF restrictions
NEXT_PUBLIC_X_DEVELOPER_TOKEN=

# URL to fun DB in — see backend /gtp repo — (format: postgresql://username:password@host:port/database)
FUN_DATABASE_URL=

# Airtable API key for Notifications & other data
AIRTABLE_API_KEY=

# Gemini API key for labels project website profiler and contract classifier
GEMINI_API_KEY=
# Optional Gemini model override for profiler (default: gemini-2.0-flash)
GEMINI_MODEL=gemini-2.0-flash

# URL of the oli-csv-classifier service for AI contract classification
# Local dev: http://localhost:3001 — Production: https://your-classifier-domain.com
CLASSIFIER_API_URL=http://localhost:3001

# GitHub credentials/config for labels project contribution PRs
OLI_GITHUB_TOKEN=
# Optional owner for contribution branches/PRs. Leave empty to use authenticated user.
OLI_GITHUB_TARGET_OWNER=
OLI_GITHUB_AUTO_CREATE_FORK=true
OLI_GITHUB_BRANCH_PREFIX=oli-contrib

# Optional repo overrides for project YAML and logos PR destinations
OLI_PROJECTS_REPO_OWNER=opensource-observer
OLI_PROJECTS_REPO_NAME=oss-directory
OLI_PROJECTS_REPO_BASE_BRANCH=main
OLI_LOGOS_REPO_OWNER=growthepie
OLI_LOGOS_REPO_NAME=gtp-dna
OLI_LOGOS_REPO_BASE_BRANCH=main

# FIGMA_ACCESS_TOKEN is used to fetch the latest icons from Figma by /lib/icons.mjs
FIGMA_ACCESS_TOKEN=
FIGMA_FILE_KEY=

# Optimism vote endpoint for RPGF3
OPTIMISM_VOTE_ENDPOINT=https://vote.optimism.io/graphql
